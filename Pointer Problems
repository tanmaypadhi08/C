#include <stdio.h>

int main()
{
    int a[5] ={10,20,30,40,50};
    int *p;
    p=a;
    printf("%d ",*p);
    printf("%d ",*(p+1));
    printf("%d ",*(p+2));

    printf("%d ",*p+1);
    printf("%d ",*p+2);
    
    printf(" incrementation ");
    printf("%d ",*p++);
    printf("%d ",++*p);
return 0;
    
}
